CLANG_CXX_LANGUAGE_STANDARD = c++0x
CLANG_CXX_LIBRARY = libc++
FRAMEWORK_SEARCH_PATHS = $(inherited) "$(SDKROOT)/Developer/Library/Frameworks"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers" "${PODS_ROOT}/Headers/Cedar" "${PODS_ROOT}/Headers/Expecta" "${PODS_ROOT}/Headers/GHUnit" "${PODS_ROOT}/Headers/Kiwi" "${PODS_ROOT}/Headers/OCMock"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers" -isystem "${PODS_ROOT}/Headers/Cedar" -isystem "${PODS_ROOT}/Headers/Expecta" -isystem "${PODS_ROOT}/Headers/GHUnit" -isystem "${PODS_ROOT}/Headers/Kiwi" -isystem "${PODS_ROOT}/Headers/OCMock"
OTHER_LDFLAGS = -ObjC -lc++ -framework Foundation -framework QuartzCore -framework XCTest
PODS_ROOT = ${SRCROOT}/Pods